source 'http://rubygems.org'

gem 'rails', '3.2.11'

gem 'pg'

# Gems used only for assets and not required
# in production environments by default.
group :assets do
  gem 'coffee-rails'
  gem 'uglifier'
end

gem 'jquery-rails'
gem 'haml'
gem 'sass-rails'

# Use unicorn as the web server
gem 'unicorn'

# To use debugger
# gem 'ruby-debug19', :require => 'ruby-debug'

group :development do
  gem 'heroku'
  gem 'foreman'
end

